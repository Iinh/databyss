{
  "name": "databyss-org-workspace",
  "version": "1.1.1",
  "description": "massive open-source citation database platform",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/databyss-org/databyss.git"
  },
  "author": "databyss-org",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:notes": "NPM_DEPLOY_TARGET=NOTES_APP yarn build && NPM_BUILD_TARGET=NOTES_APP node scripts/start.js",
    "start:login": "NPM_BUILD_TARGET=LOGIN_APP PUBLIC_URL=/login node scripts/start.js",
    "start:docs": "cd ./packages/databyss-documentation/website && yarn run start",
    "start:server": "NPM_DEPLOY_TARGET=API_SERVER NPM_BUILD_TARGET=API_SERVER NODE_ENV=development nodemon packages/databyss-api/server.js",
    "build:login": "node scripts/build.js",
    "metro:notes": "react-native start --reset-cache --config packages/databyss-notes-native/metro.config.js",
    "build": "bash scripts/build.sh",
    "build:ui": "NODE_ENV=production BABEL_ENV=production webpack --config packages/databyss-ui/webpack.config.js --mode=production",
    "test": "node scripts/test.js",
    "flow": "flow",
    "styleguide": "styleguidist server --config ./config/styleguide.config.js",
    "styleguide:build": "styleguidist build --config ./config/styleguide.config.js",
    "storybook": "start-storybook -h 0.0.0.0 -p 6006 -s ./packages/databyss-ui/public",
    "storybook:build": "build-storybook -o build -s ./packages/databyss-ui/public && node scripts/sb-publish.js Databyss",
    "sync-versions": "node scripts/sync-versions.js",
    "lint": "eslint . --cache && echo \"eslint: no lint errors\""
  },
  "jest": {
    "projects": [
      {
        "displayName": "lint",
        "runner": "jest-runner-eslint",
        "testMatch": [
          "<rootDir>/packages/**/*.js"
        ]
      },
      {
        "displayName": "test",
        "collectCoverageFrom": [
          "**/*.js"
        ],
        "resolver": "jest-pnp-resolver",
        "setupFiles": [
          "<rootDir>/config/polyfills.js"
        ],
        "setupFilesAfterEnv": [
          "<rootDir>/config/jest/registerRequireContextHook.js"
        ],
        "testMatch": [
          "<rootDir>/**/*.test.js"
        ],
        "testEnvironment": "jsdom",
        "testURL": "http://localhost",
        "transform": {
          "^.+\\.jsx?$": "<rootDir>/config/jest/babelTransform.js",
          "^(?!.*\\.(js|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
          "node_modules/(?!(react-native-web)/)"
        ],
        "moduleFileExtensions": [
          "js",
          "json"
        ]
      },
      {
        "displayName": "checkversion",
        "runner": "./tests/checkversion.runner.js",
        "testMatch": [
          "<rootDir>/**/package.json"
        ],
        "testPathIgnorePatterns": [
          "/node_modules/"
        ]
      }
    ]
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@sendgrid/mail": "^6.4.0",
    "axios": "^0.19.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "docusaurus": "^1.12.0",
    "eslint-config-react-app": "^4.0.1",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "jest-dev-server": "^4.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.5",
    "sass-loader": "^7.1.0",
    "supertest": "^4.0.2",
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@storybook/addon-options": "^5.1.1",
    "@storybook/addon-storyshots": "^5.1.1",
    "@storybook/addons": "^5.1.1",
    "@storybook/react": "^5.1.1",
    "@storybook/react-native": "^5.1.9",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-emotion": "^10.0.13",
    "babel-preset-react-app": "^9.0.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "copy-webpack-plugin": "^4.5.2",
    "csvtojson": "^2.0.8",
    "dotenv": "^8.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^4.0.0",
    "flow-bin": "^0.79.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "is-wsl": "^2.0.0",
    "jest": "^24.8.0",
    "jest-pnp-resolver": "^1.0.1",
    "jest-resolve": "^23.6.0",
    "jest-runner-eslint": "^0.7.1",
    "lorem-ipsum": "^1.0.6",
    "metro-config": "^0.55.0",
    "metro-react-native-babel-preset": "^0.55.0",
    "mini-css-extract-plugin": "^0.7.0",
    "object-assign": "4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pnp-webpack-plugin": "^1.5.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "1.13.5",
    "prettier-eslint": "^8.8.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "16.8.6",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-flexbox-grid": "^2.1.2",
    "react-google-login": "^5.0.4",
    "react-jss": "^8.6.1",
    "react-native": "^0.60.4",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-styleguidist": "^9.1.5",
    "react-svg-loader": "^3.0.3",
    "react-test-renderer": "^16.6.0",
    "resolve": "^1.11.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "0.6.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-node-externals": "^1.7.2",
    "whatwg-fetch": "2.0.3",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
